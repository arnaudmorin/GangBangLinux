GtkScrollbar {
-GtkScrollbar-has_secondary_backward_stepper:0;
-GtkScrollbar-has_secondary_forward_stepper:0;
-GtkScrollbar-has_backward_stepper:1;
-GtkScrollbar-has_forward_stepper:1;
-GtkScrollbar-min_slider_length:50;
-GtkRange-slider-width:4;
-GtkRange-trough-under-steppers:0;
-GtkRange-activate-slider:1;
-GtkRange-arrow-displacement-x:0;
-GtkRange-arrow-displacement-y:0;
-GtkRange-arrow-scaling:0.0;
-GtkRange-stepper-size:9;
-GtkRange-stepper-spacing:0;
-GtkRange-trough-border:0;
-GtkRange-shadow-type:none;
background-color:transparent;}

GtkScrollbar.trough{
background-color:transparent;}

GtkScrollbar.slider{
background-color:#e04613;}

GtkScrollbar.button{
background-color:#292929;}

GtkScrollbar.button:insensitive{
background-color:#e04613;}
