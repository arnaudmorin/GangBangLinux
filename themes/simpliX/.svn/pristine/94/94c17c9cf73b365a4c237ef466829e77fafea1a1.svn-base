<?xml version="1.0"?>

<metacity_theme>
<info>
  	<name>simpliX</name>
  	<author>SSF</author>
  	<copyright>CC-BY-SA</copyright>
  	<date>September 12, 2013</date>
  	<description>A Marco theme</description>
</info>

<frame_geometry name="frame_geometry_normal" title_scale="medium" rounded_top_left="false" rounded_top_right="false"  rounded_bottom_left="false" rounded_bottom_right="false">
 	<distance name="left_width" value="1"/>
	<distance name="right_width" value="1"/>
	<distance name="bottom_height" value="1"/>
	<distance name="left_titlebar_edge" value="5"/>
	<distance name="right_titlebar_edge" value="5"/>
	<distance name="button_width" value="13"/>
	<distance name="button_height" value="13"/>
	<distance name="title_vertical_pad" value="5"/>
	<border name="title_border" left="1" right="1" top="1" bottom="0"/>
	<border name="button_border" left="1" right="1" top="1" bottom="0"/>
</frame_geometry>

<frame_geometry name="geometry_maximized" rounded_top_left="false" rounded_top_right="false" rounded_bottom_left="false" rounded_bottom_right="false">
	<distance name="left_width" value="0"/>
	<distance name="right_width" value="0"/>
	<distance name="bottom_height" value="0"/>
	<distance name="left_titlebar_edge" value="5"/>
  	<distance name="right_titlebar_edge" value="5"/>
  	<distance name="button_width" value="13"/>
  	<distance name="button_height" value="13"/>
  	<distance name="title_vertical_pad" value="5"/>
	<border name="title_border" left="1" right="1" top="1" bottom="0"/>
	<border name="button_border" left="1" right="1" top="1" bottom="0"/>
</frame_geometry>

<frame_geometry name="border" has_title="false">
	<distance name="left_width" value="2"/>
	<distance name="right_width" value="2"/>
	<distance name="bottom_height" value="2"/>
	<distance name="left_titlebar_edge" value="1"/>
  	<distance name="right_titlebar_edge" value="1"/>
  	<distance name="button_width" value="0"/>
  	<distance name="button_height" value="0"/>
  	<distance name="title_vertical_pad" value="0"/>
  	<border name="title_border" left="0" right="0" top="0" bottom="0"/>
 	<border name="button_border" left="3" right="0" top="0" bottom="0"/>
</frame_geometry>


<draw_ops name="draw_title_text_normal">
	<title color="#fff" x="3 `max` (width-title_width)/2" y="0 `max` ((height-title_height)/2)"/>
</draw_ops>

<draw_ops name="draw_title_text_inactive">
	<title color="#888" x="3 `max` (width-title_width)/2" y="0 `max` ((height-title_height)/2)"/>
</draw_ops>

<draw_ops name="draw_title">
	<rectangle color="#dadada" filled="true"
	           x="0" y="0"
	           width="width"
	           height="height"/>
	<rectangle color="#292929" filled="true"
	           x="1" y="1"
	           width="width-left_width-right_width"
	           height="height"/>
</draw_ops>

<draw_ops name="draw_title_inactive">
	<rectangle color="#dadada" filled="true"
	           x="0" y="0"
	           width="width"
	           height="height"/>
	<rectangle color="#292929" filled="true"
	           x="1" y="1"
	           width="width-left_width-right_width"
	           height="height"/>
</draw_ops>

<draw_ops name="draw_title_maximized">
	<rectangle color="#292929" filled="true"
	           x="0" y="0"
	           width="width"
	           height="height"/>
</draw_ops>

<draw_ops name="draw_title_maximized_inactive">
	<rectangle color="#292929" filled="true"
	           x="0" y="0"
	           width="width"
	           height="height"/>
</draw_ops>

<!-- Window Frames -->

<draw_ops name="frame-bottom-focused">
<rectangle color="#dadada" filled="true"
	   x="0" y="0"
	   width="width"
	   height="height"/>
</draw_ops>
<draw_ops name="frame-bottom-unfocused">
<rectangle color="#dadada" filled="true"
	   x="0" y="0"
	   width="width"
	   height="height"/>
</draw_ops>

<draw_ops name="frame-left-focused">
<line color="#dadada"
        x1="0"
        y1="0"
        x2="0"
        y2="height"
        width="1"/>
</draw_ops>
<draw_ops name="frame-left-unfocused">
<line color="#dadada"
        x1="0"
        y1="0"
        x2="0"
        y2="height"
        width="1"/>
</draw_ops>

<draw_ops name="frame-right-focused">
<line color="#dadada"
        x1="0"
        y1="0"
        x2="0"
        y2="height"
        width="1"/>
</draw_ops>
<draw_ops name="frame-right-unfocused">
<line color="#dadada"
        x1="0"
        y1="0"
        x2="0"
        y2="height"
        width="1"/>
</draw_ops>

	<draw_ops name="menu_focused_normal">
   		<image filename="images/menu-active.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="menu_focused_prelight">
  	 <image filename="images/menu-prelight.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="menu_focused_pressed">
		<image filename="images/menu-pressed.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="menu_unfocused_normal">
  	 	<image filename="images/menu-inactive.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="menu_unfocused_prelight">
   		<image filename="images/menu-prelight.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="menu_unfocused_pressed">
		<image filename="images/menu-pressed.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="minimize_focused_normal">
		<image filename="images/hide-active.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="minimize_focused_prelight">
		<image filename="images/hide-prelight.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="minimize_focused_pressed">
		<image filename="images/hide-pressed.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="minimize_unfocused_normal">
		<image filename="images/hide-inactive.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="minimize_unfocused_prelight">
		<image filename="images/hide-prelight.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="minimize_unfocused_pressed">
		<image filename="images/hide-pressed.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="maximize_focused_normal">
		<image filename="images/maximize-active.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="maximize_focused_prelight">
		<image filename="images/maximize-prelight.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="maximize_focused_pressed">
		<image filename="images/maximize-pressed.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="maximize_unfocused_normal">
		<image filename="images/maximize-inactive.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="maximize_unfocused_prelight">
		<image filename="images/maximize-prelight.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="maximize_unfocused_pressed">
		<image filename="images/maximize-prelight.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="unmaximize_focused_normal">
		<image filename="images/maximize-toggled-active.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
