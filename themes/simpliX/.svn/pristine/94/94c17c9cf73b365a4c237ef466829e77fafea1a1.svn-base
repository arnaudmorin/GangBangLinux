		<image filename="images/maximize-toggled-prelight.xpm" x="0" y="0" width="object_width" height="object_height"/>
	</draw_ops>
	<draw_ops name="unmaximize_focused_pressed">
		<image filename="images/maximize-toggled-pressed.xpm" x="0" y="0" width="object_width" height="object_height"/>
