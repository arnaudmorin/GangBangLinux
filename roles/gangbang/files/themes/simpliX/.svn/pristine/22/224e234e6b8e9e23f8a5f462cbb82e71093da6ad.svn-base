style"gtkmenubar"
{
xthickness=0
ythickness=0
GtkMenuBar::shadow-type=GTK_SHADOW_NONE
GtkMenuBar::internal-padding=4
fg[NORMAL]="#fff"
bg[NORMAL]="#292929"
bg[PRELIGHT]="#e04613"
engine"pixmap"
{
image
{
function=SHADOW
shadow=NONE
file="../images/menubarshadow.gif"
border={0,0,1,3}
stretch=TRUE
}
}
}
widget_class"*.<GtkMenuBar>*"style"gtkmenubar"
style"gtkmenubarfont"
{
GtkMenuBar::shadow-type=GTK_SHADOW_NONE
GtkMenuBar::internal-padding=3
fg[PRELIGHT]="#fff"
fg[NORMAL]="#fff"
engine"mist"{}}
widget_class"*.<GtkMenuBar>*<GtkLabel>*"style"gtkmenubarfont"
